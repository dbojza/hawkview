<script>
	function _pushToStatistic(fncArg)
	{
		var cnfObj = 
		{
			item : false,	// item object
			json : false,	// json object
		};
		for(var key in fncArg)
		{
			if( fncArg.hasOwnProperty(key) )
			{
				cnfObj[key] = fncArg[key];
			}
		}

		// parent
		var _func = this;

		_func._pushName = false;
		_func._pushData = false;

		/*
		*  helper fnc: set pusshing data
		*/
		_func.setPushingData = function()
		{
			if( _func._pushName && _func._pushData )
			{
				if( typeof window[_func._pushName] !== 'object' )
				{
					window[_func._pushName] = [];
				}
				window[_func._pushName].push
				(
					_func._pushData
				);

				if( typeof help == 'object' && typeof help.ajax == 'function' && typeof _func._pushData.event == 'string' )
				{
					help.ajax(
					{
						type: 'post',
						url: jsConf.ajaxUrl + '?action=log',
						dataType: 'json',
						data: {'data': JSON.stringify(_func._pushData)},
					});
				}
			}
		}

		/*
		*  helper fnc: get general data
		*/
		_func.getGeneralData = function(key)
		{
			if( _func._pushName && typeof window[_func._pushName] == 'object' )
			{
				_returnData = false;

				window[_func._pushName].forEach(function(data)
				{
					if( typeof data['generalData'] == 'boolean' )
					{
						if( typeof key !== 'undefined' )
						{
							if( typeof data[key] !== 'undefined' )
							{
								_returnData = data[key];
							}
						}
						else
						{
							_returnData = data;
						}
					}
				});

				return _returnData;
			}
		}

		/*
		*  inner fnc: product click
		*/
		_func.eccProductClick = function()
		{
			var _sendData = 
			{
				ecommerce : 
				{
					click : 
					{
						actionField: (typeof cnfObj.json.data.actionField == 'object') ? cnfObj.json.data.actionField : {},
						products: (typeof cnfObj.json.data.products == 'object') ? cnfObj.json.data.products : []
					}
				},
				event : 'productClick'
			};

			return _sendData;
		}
		
		/*
		*  inner fnc: product impressions
		*/
		_func.eccProductImpressions = function()
		{
			var _sendData = 
			{
				ecommerce : 
				{
					currencyCode: _func.getGeneralData('currencyCode'),
					impressions : cnfObj.json.data
				}
			};
			return _sendData;
		}

		/*
		*  inner fnc: product detail
		*/
		_func.eccProductDetail = function()
		{
			var _sendData = 
			{
				ecommerce : 
				{
					detail : 
					{
						actionField: (typeof cnfObj.json.data.actionField == 'object') ? cnfObj.json.data.actionField : {},
						products: (typeof cnfObj.json.data.products == 'object') ? cnfObj.json.data.products : []
					}
				}
			};
			return _sendData;
		}

		/*
		*  inner fnc: promotion click
		*/
		_func.eccPromotionClick = function()
		{
			var _sendData = 
			{
				ecommerce : 
				{
					promoClick :
					{
						promotions: (typeof cnfObj.json.data.promotions == 'object') ? cnfObj.json.data.promotions : []
					}
				},
				event : 'promotionClick'
			};
			return _sendData;
		}

		/*
		*  inner fnc: promotion impressions
		*/
		_func.eccPromotionImpressions = function()
		{
			var _sendData = 
			{
				ecommerce : 
				{
					promoView :
					{
						promotions : cnfObj.json.data
					}
				}
			};
			return _sendData;
		}

		/*
		*  inner fnc: product impressions
		*/
		_func.eccBasketManage = function()
		{
			var _form = cnfObj.item;

			if( _form.tagName !== 'FORM' )
			{
				return false;
			}

			var _formIden = (typeof _form.elements['_formId'] == 'object') ? _form.elements['_formId'].value : false, 
				_prevPice = (typeof _form.elements['prevPice'] == 'object') ? _form.elements['prevPice'].value : false,
				_sendPice = (typeof _form.elements['piece'] == 'object') ? _form.elements['piece'].value : false;

			_sendPice = (!isNaN(_sendPice) && _sendPice > 0) ? _sendPice : false;
			_prevPice = (!isNaN(_prevPice) && _prevPice > 0) ? _prevPice : false;

			if( _formIden == 'basket/update' && _prevPice > 0 && _sendPice > 0 )
			{
				_sendPice = _sendPice - _prevPice;
			}
			if( _formIden == 'basket/delete' )
			{
				_sendPice = _sendPice * -1;
			}
			if( _sendPice == 0 )
			{
				return false;
			}

			if( _sendPice > 0 )
			{
				cnfObj.json.data.products.quantity = _sendPice;

				var _sendData = 
				{
					event : 'insertIntoCart',
					ecommerce : 
					{
						add : 
						{
							products: (typeof cnfObj.json.data.products == 'object') ? cnfObj.json.data.products : []
						}
					}
				};
				return _sendData;
			}
			else
			{
				cnfObj.json.data.products.quantity = _sendPice * - 1;

				var _sendData = 
				{
					event : 'deleteFromCart',
					ecommerce : 
					{
						remove :
						{
							products: (typeof cnfObj.json.data.products == 'object') ? cnfObj.json.data.products : []
						}
					}
				};
				return _sendData;
			}
		}

		/*
		*  inner fnc: checkout step
		*/
		_func.eccCheckoutStep = function()
		{
			var _sendData = 
			{
				ecommerce : 
				{
					checkout : 
					{
						actionField: (typeof cnfObj.json.data.actionField == 'object') ? cnfObj.json.data.actionField : {},
						products: (typeof cnfObj.json.data.products == 'object') ? cnfObj.json.data.products : []
					}
				},
				event : 'checkout',
			};
			return _sendData;
		}

		/*
		*  inner fnc: purchase
		*/
		_func.eccPurchase = function()
		{
			var _sendData = 
			{
				ecommerce : 
				{
					purchase : 
					{
						actionField: (typeof cnfObj.json.data.actionField == 'object') ? cnfObj.json.data.actionField : {},
						products: (typeof cnfObj.json.data.products == 'object') ? cnfObj.json.data.products : []
					}
				},
				event : 'purchase',
			};
			return _sendData;
		}

		// run push 
		if( typeof cnfObj.json.pushIn == 'object' )
		{
			var _pushIn = cnfObj.json.pushIn;
			var _funcIn = cnfObj.json.funcIn;

			_pushIn.forEach(function(pushName)
			{
				_func._pushName = pushName;

				if( typeof _funcIn == 'object' )
				{
					_funcIn.forEach(function(funcName)
					{
						if( typeof this[funcName] == 'function' )
						{
							_func._pushData = this[funcName]();

							_func.setPushingData();
						}
					});
				}
				else
				{
					_func._pushData = cnfObj.json.data;

					_func.setPushingData();
				}
			});
		}
	}
</script>






